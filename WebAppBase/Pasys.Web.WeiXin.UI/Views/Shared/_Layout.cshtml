<!DOCTYPE html>
<html>
<head>
    <title>@WorkContext.GlobalConfig.SiteTitle</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="description" content="@WorkContext.GlobalConfig.SEODescription">
    <link rel="stylesheet" href="@Url.AreaContent("~/Contents/jqueryweui/lib/weui.min.css")">
    <link rel="stylesheet" href="@Url.AreaContent("~/Contents/jqueryweui/css/jquery-weui.css")">
    <link rel="stylesheet" href="@Url.AreaContent("~/Contents/demo/css/demos.css")">

    @RenderSection("Style", false)
    @RenderSection("PrepareScript", false)


</head>
<body ontouchstart>
    @RenderBody()    
    <script src="@Url.AreaContent("~/Contents/jqueryweui/lib/jquery-2.1.4.js")"></script>
    <script src="@Url.AreaContent("~/Contents/jqueryweui/lib/fastclick.js")"></script>
    <script>
        $(function () {
            FastClick.attach(document.body);
        });
    </script>
    <script src="@Url.AreaContent("~/Contents/jqueryweui/js/jquery-weui.js")"></script>
    <script src="@Url.AreaContent("~/Contents/jqueryweui/js/city-picker.js")"></script>
    <script src="@Url.AreaContent("~/Contents/jqueryweui/js/swiper.js")"></script>

    @RenderSection("Script", required: false)
</body>
</html>
